{
  "name": "jekyll-webpack-template",
  "version": "1.1.0",
  "description": "",
  "scripts": {
    "dev": "bundle exec jekyll serve --livereload",
    "dev-webpack": "yarn copy-assets && webpack --watch",
    "copy-assets": "rsync _assets/ assets/ --exclude=.DS_Store -av --delete",
    "dist": "export NODE_ENV=production; yarn copy-assets && webpack && bundle exec jekyll build",
    "deploy": "yarn dist && firebase deploy"
  },
  "author": "@stresslimit",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "css-layout": "stresslimit/css-layout#1.2.3",
    "css-loader": "^3.5.3",
    "csswring": "^7.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-calc": "^7.0.2",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.1.1",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "yarn": "^1.22.4"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0"
  }
}
